#include <thread>
#include <iostream>
using namespace std;

//sum every element of an array
uint64_t test_r_sequential(uint64_t a[], int n) {
	uint64_t sum = 0;
	for (uint64_t i = 0; i < n; i++)
		sum += a[i];
	return sum;
}

//increment every element of an array (read and write)
uint64_t test_rw_sequential(uint64_t a[], int n) {
	for (uint64_t i = 0; i < n; i++)
		a[i]++;
	return 0;
}


// sum elements out of order
uint64_t test_rw_stride(uint64_t a[], int stride, int n) {
	uint64_t sum = 0;
	for (uint64_t j = 0; j < stride; j++)
		for (uint64_t i = 0; i < n; i+= stride)
			sum += a[i];
	return 0;
}




int main() {

}
